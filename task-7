#include <iostream>
using namespace std;
void fibonacci(int n){
    int a = 0, b = 1, c;
    for (int i = 1; i <= n; i++) {
        cout << a << endl;   
        c = a + b;          
        a = b;
        b = c;
    }
}
int main() {
    int n;
    cout << "Neçə ədəd istədiyinizi daxil edin:";
    cin >> n;
    
    cout << "Fibonacci ardıcıllığı:"<<endl;
    fibonacci(n);
    
return 0;
}
